{
  "presets": [
    [
      "@babel/env",
      {
        "corejs": 2,
        "modules": false,
        "targets": {
          "browsers": [ "> 1%", "last 10 versions"],
          "uglify": true
        },
        "useBuiltIns": "entry"
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-class-properties"
  ]
}
